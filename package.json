{
  "name": "bluebird",
  "description": "Full featured Promises/A+ implementation with exceptionally good performance",
  "version": "3.4.7",
  "keywords": [
    "promise",
    "performance",
    "promises",
    "promises-a",
    "promises-aplus",
    "async",
    "await",
    "deferred",
    "deferreds",
    "future",
    "flow control",
    "dsl",
    "fluent interface"
  ],
  "scripts": {
    "lint": "node scripts/jshint.js",
    "test": "node tools/test.js",
    "istanbul": "istanbul",
    "prepublish": "npm run generate-browser-core && npm run generate-browser-full",
    "generate-browser-full": "node tools/build.js --no-clean --no-debug --release --browser --minify",
    "generate-browser-core": "node tools/build.js --features=core --no-debug --release --zalgo --browser --minify && mv js/browser/bluebird.js js/browser/bluebird.core.js && mv js/browser/bluebird.min.js js/browser/bluebird.core.min.js"
  },
  "homepage": "https://github.com/petkaantonov/bluebird",
  "repository": {
    "type": "git",
    "url": "git://github.com/petkaantonov/bluebird.git"
  },
  "bugs": {
    "url": "http://github.com/petkaantonov/bluebird/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Petka Antonov",
    "email": "petka_antonov@hotmail.com",
    "url": "http://github.com/petkaantonov/"
  },
  "devDependencies": {
    "acorn": "~4.0.11",
    "baconjs": "^0.7.43",
    "bluebird": "^3.5.0",
    "body-parser": "^1.10.2",
    "browserify": "^14.1.0",
    "cli-table": "~0.3.1",
    "co": "^4.2.0",
    "cross-spawn": "^5.1.0",
    "glob": "^7.1.1",
    "grunt-saucelabs": "~9.0.0",
    "highland": "^2.3.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.6.0",
    "jshint-stylish": "~2.2.1",
    "mkdirp": "~0.5.0",
    "mocha": "~3.2.0",
    "open": "~0.0.5",
    "optimist": "~0.6.1",
    "rimraf": "~2.6.1",
    "rx": "^4.1.0",
    "serve-static": "^1.7.1",
    "sinon": "~1.17.7",
    "uglify-js": "~2.8.7",
    "kefir": "^3.7.1"
  },
  "readmeFilename": "README.md",
  "main": "./js/release/bluebird.js",
  "browser": "./js/browser/bluebird.js",
  "files": [
    "js/browser",
    "js/release",
    "LICENSE"
  ]
}
